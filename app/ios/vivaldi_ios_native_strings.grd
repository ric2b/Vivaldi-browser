      <message name="IDS_VIVALDI_TAB_GRID_REGULAR_TABS_EMPTY_TITLE" desc="Title shown above a message on the regular tab switcher when there are no tabs.">
        Tabs
      </message>
      <message name="IDS_VIVALDI_TAB_GRID_REGULAR_TABS_EMPTY_MESSAGE" desc="Message shown underneath a title on the regular tab switcher when there are no tabs, explaining why to use the regular tab switcher.">
        Tap the plus icon below to create a new tab.
      </message>
      <message name="IDS_VIVALDI_TAB_SWITCHER_RECENTLY_CLOSED_TABS_EMPTY_TITLE" desc="Title shown above a message on the recently closed tab switcher when there are no items.">
        Recently Closed Tabs
      </message>
      <message name="IDS_VIVALDI_TAB_SWITCHER_RECENTLY_CLOSED_TABS_EMPTY_MESSAGE" desc="Message shown underneath a title on the recently closed tab switcher when there are no item.">
        Tabs you recently closed will appear here.
      </message>
      <message name="IDS_VIVALDI_TAB_SWITCHER_RECENTLY_CLOSED_TABS_UNAVAILABLE_MESSAGE" desc="Message shown in the Recently Closed Tabs page of the tab grid when the IncognitoModeAvailability enterprise policy is set to forced.">
        Your organization requires you to browse privately. Tabs aren't saved in Private mode.
        <ph name="BEGIN_LINK">BEGIN_LINK</ph>Learn more<ph name="END_LINK">END_LINK</ph>
      </message>
      <message name="IDS_VIVALDI_TAB_SWITCHER_RECENTLY_CLOSED_TABS_UNAVAILABLE_TITLE" desc="Title shown above a message in the Recently Closed Tabs page of the tab grid when the IncognitoModeAvailability enterprise policy is set to forced.">
        Recently Closed Tabs are Unavailable
      </message>
      <message name="IDS_VIVALDI_TAB_GRID_INCOGNITO_TABS_EMPTY_MESSAGE" desc="Message shown in the Incognito Tabs page when the grid is empty, explaining that the user can open a new tab.">
        Searches, sites visited, cookies, and temporary files won't be stored by Vivaldi when using Private Tabs.
      </message>
      <message name="IDS_VIVALDI_TAB_GRID_INCOGNITO_TABS_EMPTY_TITLE" desc="Title shown above a message on the Incognito tab switcher when there are no Incognito tabs. [iOS only]">
        Private Tabs
      </message>
      <message name="IDS_VIVALDI_TAB_SWITCHER_SYNC_TABS_EMPTY_TITLE" desc="Title shown above a message on the synced tab switcher when there are no items. [iOS only]">
        No Synced Tabs
      </message>
      <message name="IDS_VIVALDI_TAB_SWITCHER_SYNC_TABS_EMPTY_MESSAGE" desc="Message shown underneath a title on the synced tab switcher when there are no item.">
        You currently have no tabs synchronized from other devices.
      </message>
      <message name="IDS_VIVALDI_TAB_SWITCHER_SYNC_TABS_UNAUTH_TITLE" desc="Title shown above a message on the synced tab switcher when the user is not logged in. [iOS only]">
        Synced Tabs
      </message>
      <message name="IDS_VIVALDI_TAB_SWITCHER_SYNC_TABS_UNAUTH_MESSAGE" desc="Message shown underneath a title on the synced tab switcher when user is not logged in. [iOS only]">
        Synchronized tabs from other devices will appear here. Sign in to Sync to use this service.
      </message>
      <message name="IDS_VIVALDI_TAB_SWITCHER_SYNC_TABS_ENABLE_SYNC_MESSAGE" desc="Message shown underneath a title on the synced tab switcher when user is logged in but sync is disabled. [iOS only]">
